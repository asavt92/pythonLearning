    "# Сопрограммы (корутины)\n",
    "\n",
    "def grep(pattern):\n",
    "    print(\"start grep\")\n",
    "    while True:\n",
    "        line = yield\n",
    "        if pattern in line:\n",
    "            print(line)\n",
    "\n",
    "g = grep(\"python\")\n",
    "next(g) # g.send(None)\n",
    "g.send(\"golang is better?\")\n",
    "g.send(\"python is simple!\")\n"
   ]
